    <div class="container my-5">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <a href="student/create" class="btn btn-primary">Add New Student</a>
                <br><br>
                <div class="card">
                    <div class="card-body">
                        <h2>All Students</h2>
                        <hr>

                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Name</th>
                                    <th>email</th>
                                    <th>cell</th>
                                    <th>photo</th>
                                    <th>action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% students.forEach((element, index) => { %>
                                    <tr>
                                        <td><%= index + 1 %></td>
                                        <td><%= element.name %> </td>
                                        <td><%= element.email %></td>
                                        <td><%= element.cell %></td>
                                        <td>
                                            <img style="width: 50px; height: 50px; object-fit: cover;" src="assets/upload/<%= element.photo %>" alt="">
                                        </td>
                                        <td>
                                            <a href="/student/<%= element._id %>" class="btn btn-s btn-info"><i class="fa fa-eye"></i></a>
                                            <a href="/student/edit/<%= element._id %>" class="btn btn-s btn-warning"><i class="fa fa-edit"></i></a>
                                            <a href="student/delete/<%= element._id %>" class="btn btn-s btn-danger"><i class="fa fa-trash"></i></a>
                                            
                                        </td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
